<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">
            <action name="pdfinvoices_order">
                <settings>
                    <url path="printpdf/sales/printinvoices"/>
                    <type>pdfinvoices_order</type>
                    <label translate="true">Download PDF Invoice</label>
                </settings>
            </action>
            <action name="pdf_orders">
                <settings>
                    <url path="printpdf/sales/printorders"/>
                    <type>pdf_orders</type>
                    <label translate="true">Print Orders</label>
                </settings>
            </action>
            <action name="pdf_shipments">
                <settings>
                    <url path="printpdf/sales/downloadshipment"/>
                    <type>pdf_shipment</type>
                    <label translate="true">Print Shipments</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>